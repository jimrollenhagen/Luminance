@font-face {
    font-family: caviar;
    src: url('fonts/CaviarDreams.ttf'); }

/* Layout (Torrents page) */

.cats_col { padding: 0; }
.category { width: 28px; height: 28px; margin: 0; padding: 0; }

/* Global */
* {
    padding: 0;
    margin: 0;
    outline: 0pt none;
}

:focus {
    outline: 0pt none;
}
html {
    width: 100%;
}


body {
    width: 100%;
    /*background: #202020 url('http://jerking.empornium.ph/images/2015/06/18/osbg37669.jpg');*/
    background: #202020     url('http://jerking.empornium.ph/images/2015/10/29/bg_black.jpg')repeat;
    #background-size: cover;
    background-attachment: fixed;
    font-family: caviar;
    font-weight: normal;
    color: #DADADA;
}

#logo {
    width: 200px;
    height: 70px;
    background: url('http://jerking.empornium.ph/images/2015/10/29/emporniumf1300.png') no-repeat center center;
    position: absolute;
    margin-top:-30px;
    top:85%;
    left: 15px;
}
#logo a {
    border: none;
    width: 100%;
    height: 100%;
    display: block;
}
#logo::after {
    width: 94%;
    margin: -14px 0 0 10px;
    text-align:center;
    font-size:0.7em;
    color:#CAEBF9;
    display: none;
}


/* Common elements */

a , .torrent_table .linkbox a {
    color:#C5B358;
    text-decoration: none;
    border: none;
}
a:visited, .torrent_table .linkbox a:visited {
    color:#62592C;
}
a:hover, .torrent_table .linkbox a:hover {
    color: #D6C98A !important;
    text-decoration: underline;

}

tr.smallhead a, .cat_list a, tr.smallhead a:visited, .cat_list a:visited {
