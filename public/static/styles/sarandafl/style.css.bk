        background-repeat: no-repeat;
        animation: ani_major_stats_off 600ms linear;
         -webkit-animation: ani_major_stats_off 600ms linear;
        transition: background-color 600ms;
         -webkit-transition: background-color 600ms;
}



#userinfo_username, #userinfo_tools {
    display:inline;
    position: relative;
}
li#nav_userinfo, #userinfo_tools{
    padding-bottom:6px !important;
}
#userinfo_username li ul, #userinfo_tools li ul,#user_dropdown li ul {
    list-style: none;
    visibility: hidden;
    position: absolute;
    left: 1px;
    top: 21px;
    background-color: #202020;
    opacity: 0;
    z-index: 10;
    border: 1px solid #202020;
    box-shadow: 4px 5px 12px -3px rgba(0,0,0,.6);
    transform-origin: 0% 0%;
    transform: scaleY(0.2);
    transition: opacity ease-in 200ms, visibility 200ms, transform ease-in 200ms;
    -webkit-transform-origin: 0% 0%;
    -webkit-transform: scaleY(0.2);
    -webkit-transition: opacity ease-in 200ms, visibility 200ms, -webkit-transform ease-in 200ms;
}
#userinfo_username li ul {
    left: 80px;
    min-width:50%;
}
li#nav_userinfo:hover ul, #userinfo_tools:hover li ul,#user_dropdown li ul:hover {
    visibility: visible;
    opacity: 1;
    transform: scaleY(1.0);
    transition: opacity ease-out 300ms .1s, transform ease-out 300ms .1s;
    -webkit-transform: scaleY(1.0);
    -webkit-transition: opacity ease-out 300ms .1s, -webkit-transform ease-out 300ms .1s;
}
#userinfo_username li ul a, #userinfo_tools li ul a,#user_dropdown li ul a {
    display: block;
}
#userinfo_username li ul li a {
    padding: 4px 6px 4px 26px;
    line-height:19px;
    background-position: 5px center;
    background-repeat: no-repeat;
    transition: background-color 400ms;
    -webkit-transition: background-color 400ms;
}
#userinfo_tools li ul li a {
    padding: 2px 6px 2px 6px;
    line-height:17px;
}
#userinfo_username li ul li:nth-child(even) a, #userinfo_tools li ul li:nth-child(even) a  {
    background-color: rgba(0,0,0,.08);
}
#userinfo_username li ul li:first-child a {
}
#userinfo_username li ul li:last-child a {
}
#userinfo_username li ul li a:hover, #userinfo_tools li ul li a:hover {
    background:rgba(255,255,255,0.4);
    background-size: 16px;
    color:#F3F3F3;
    transition: background-color 0s;
    -webkit-transition: background-color 0s;
    animation: ani_userinfo 0ms infinite linear; /* 1100ms to enable */
    -webkit-animation: ani_userinfo 0ms infinite linear;
}
@keyframes ani_userinfo {
    0% {background-size: 16px;}
   25% {background-size: 20px;}
   75% {background-size: 12px;}
  100% {background-size: 16px;}
}
@-webkit-keyframes ani_userinfo {
    0% {background-size: 16px;}
   25% {background-size: 20px;}
   75% {background-size: 12px;}
  100% {background-size: 16px;}
}


